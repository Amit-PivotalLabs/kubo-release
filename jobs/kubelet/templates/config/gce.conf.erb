<% if_p("cloud-provider") do |cloud_provider| %>
<% if <% p("cloud_provider.type") == "gce" %>
[global]
  project-id: <%= p("cloud_provider.gce.project-id") %>
  network-name: <%= %p("cloud_provider.gce.network-name") %>
  subnetwork-name: <%= p("cloud_provider.gce.subnetwork-name") %>
  <% end %>
<% end %>
